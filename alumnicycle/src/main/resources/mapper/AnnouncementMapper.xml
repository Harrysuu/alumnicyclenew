<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.elec.alumnicycle.mapper.AnnouncementMapper">

    <select id="getAnnouncementByAdminId" resultType="com.elec.alumnicycle.entity.Announcement" >
        SELECT
        *
        FROM
        announcement AS a  <!-- give life_post alias life_posts -->
        LEFT JOIN
        create_announce AS b ON b.announcement_id = a.id
        WHERE
        b.admin_id = #{adminId}
    </select>

</mapper>
